<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<mapper namespace="com.esuizhen.server.sync.dao.server.UuidRelationshipDao" >
    <select id="getUuidFinalBypatientUuid" resultType="java.lang.String">
		SELECT uuidFinal
		FROM user_db.r_uuid_relationship
		WHERE uuid=#{patientUuid}
	</select>
</mapper>